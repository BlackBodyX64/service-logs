{
  "name": "service-log-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node ./src/index.ts",
    "watch": "nodemon",
    "prod:start": "tsc && pm2 start ./dist/index.js -n \"service-log\"",
    "prod:stop": "pm2 stop \"service-log\"",
    "prod:restart": "pm2 stop \"service-log\" && tsc && pm2 start ./dist/index.js -n \"service-log\"",
    "prod:logs": "pm2 logs \"service-log\"",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d ./src/utils/DataSource.ts",
    "migration:up": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/utils/DataSource.ts",
    "migration:down": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/utils/DataSource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemailer": "^6.9.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16",
    "typeorm-extension": "^2.8.0",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.2",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.1",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
